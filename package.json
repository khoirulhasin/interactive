{
  "name": "ais_merahputih",
  "version": "1.0.0",
  "description": "AIS Merah Putih",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:postgres": "npm run prisma-merge && dotenv -e ./src/environments/.env -- npx prisma migrate dev --name postgres-init",
    "prisma-merge": "npx prisma-merge --baseFile ./src/models/base.prisma --schemaFilePatterns ./src/models/*.prisma --outputFile ./src/models/schema.prisma",
    "build": "npx tsc -p . && copyfiles ./src/environments/.env* ./lib/",
    "start": "dotenv -e ./src/environments/.env -- ts-node --esm lib/index.js",
    "dev": "npm run prisma-merge && prisma generate &&  dotenv -e ./src/environments/.env -- nodemon index.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/node": "^14.14.25",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^6.0.0",
    "dotenv-expand": "^10.0.0",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.6.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "prisma": {
    "schema": "./src/models/schema.prisma"
  }
}
