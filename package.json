{
  "name": "ais_merahputih",
  "version": "1.0.0",
  "description": "AIS Merah Putih",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:postgre": "npm run prisma-merge:postgre && dotenv -e .env -- prisma migrate dev --name postgres-init --schema ./src/models/postgre/schema.prisma",
    "prisma-merge:postgre": "prisma-merge --baseFile ./src/models/postgre/base.prisma --schemaFilePatterns ./src/models/postgre/*.prisma --outputFile ./src/models/postgre/schema.prisma",
    "prisma-merge:mongo": "prisma-merge --baseFile ./src/models/mongo/base.prisma --schemaFilePatterns ./src/models/mongo/*.prisma --outputFile ./src/models/mongo/schema.prisma",
    "build": "tsc -p . && tsc-alias -p tsconfig.json && copyfiles .env* ./lib/ && copyfiles ./src/models/mongo/**/schema.prisma ./lib/ && copyfiles ./src/models/postgre/**/schema.prisma ./lib/",
    "start": "dotenv -e .env -- ts-node --esm lib/index.js",
    "dev": "npm run prisma-merge:postgre && npm run prisma-merge:mongo && prisma generate --schema ./src/models/postgre/schema.prisma  && prisma generate --schema ./src/models/mongo/schema.prisma &&  dotenv -e .env -- nodemon index.ts",
    "dev-without-db": "dotenv -e .env -- nodemon index.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^14.14.25",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^6.0.0",
    "dotenv-expand": "^10.0.0",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.6.1",
    "prisma-merge": "^0.2.0",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.4",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
